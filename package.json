{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "fix": "chmod +x scripts/fix-common-issues.sh && ./scripts/fix-common-issues.sh",
    "build:android": "eas build --platform android --local",
    "build:android-cloud": "eas build --platform android",
    "build:ios": "eas build --platform ios --local",
    "build:ios-cloud": "eas build --platform ios",
    "build:all": "eas build --platform all --local",
    "build:all-cloud": "eas build --platform all",
    "preview": "eas build --platform android --profile preview",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "configure": "eas build:configure",
    "update": "eas update",
    "clean": "expo r -c",
    "test": "expo start --no-dev --minify"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/bottom-tabs": "^7.1.1",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.1.1",
    "expo": "~53.0.0",
    "expo-haptics": "^14.1.4",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "^0.28.17",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-card-stack-swiper": "^1.2.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~19.0.10",
    "@types/react-dom": "^18.2.55",
    "@types/react-native": "~0.73.0",
    "prettier": "^2.2.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
